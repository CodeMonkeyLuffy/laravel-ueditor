{
  "name": "codemonkeyluffy/laravel-ueditor",
  "description": "UEditor integration for Laravel.",
  "license": "MIT",
  "authors": [
    {
      "name": "codemonkeyluffy",
      "email": "codemonkeyluffy@gmail.com"
    }
  ],
  "require": {
    "aliyuncs/oss-sdk-php": "^2.4"
  },
  "autoload": {
    "psr-4": {
      "Codemonkeyluffy\\LaravelUEditor\\": "src/"
    }
  },
  "require-dev": {
    "fabpot/php-cs-fixer": "^1.10"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Codemonkeyluffy\\LaravelUEditor\\UEditorServiceProvider"
      ]
    }
  },
  "repositories": {
    "packagist": {
      "type": "composer",
      "url": "https://mirrors.aliyun.com/composer/"
    }
  }
}
